  <!-- SQL文件设置 -->
  <mappers>
    <mapper resource="com/woting/crawler/core/album/persis/sql/Album.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/audio/persis/sql/Audio.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/albumaudioref/persis/sql/AlbumAudioRef.mapper.xml"/>
