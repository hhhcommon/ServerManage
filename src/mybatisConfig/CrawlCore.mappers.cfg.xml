    <typeAlias alias="album"      type="com.woting.crawler.core.album.persis.po.AlbumPo"/>
    <typeAlias alias="audio"      type="com.woting.crawler.core.audio.persis.po.AudioPo"/>
    <typeAlias alias="albumaudioref"      type="com.woting.crawler.core.albumaudioref.persis.po.AlbumAudioRefPo"/>
    <typeAlias alias="dictm"      type="com.woting.crawler.core.dict.persis.po.DictMPo"/>
    <typeAlias alias="dictd"      type="com.woting.crawler.core.dict.persis.po.DictDPo"/>
    <typeAlias alias="dictref"    type="com.woting.crawler.core.dict.persis.po.DictRefPo"/>
    <typeAlias alias="cperson"     type="com.woting.crawler.core.cperson.persis.po.CPersonPo"/>
