    <mapper resource="com/woting/crawler/core/cperson/persis/sql/CPersonRef.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/cplaycount/persis/sql/CPlayCount.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/cfavorite/persis/sql/CFavorite.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/ccomment/persis/sql/CComment.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/csubscribe/persis/sql/CSubscribe.mapper.xml"/>
    <mapper resource="com/woting/crawler/core/cforward/persis/sql/CForward.mapper.xml"/>
