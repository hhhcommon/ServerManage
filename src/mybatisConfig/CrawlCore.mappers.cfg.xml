    <typeAlias alias="cfavorite"   type="com.woting.crawler.core.cfavorite.persis.po.CFavoritePo"/>
    <typeAlias alias="ccomment"    type="com.woting.crawler.core.ccomment.persis.po.CCommentPo"/>
